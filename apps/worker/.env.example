# Worker environment configuration
# Copy to .dev.vars, .staging.vars, or .prod.vars

# Supabase (required)
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# R2/S3 Storage (required for production)
R2_BUCKET_NAME=evidence-layer
R2_ENDPOINT=https://your-r2-endpoint.example.com
R2_ACCESS_KEY_ID=your_access_key
R2_SECRET_ACCESS_KEY=your_secret_key

# Rate limiting
RATE_LIMIT_WINDOW=60
RATE_LIMIT_MAX=100

# Captcha (Turnstile) - optional
TURNSTILE_SITE_KEY=your_site_key
TURNSTILE_SECRET_KEY=your_secret_key

# Kill switch (default: false for dev)
KILL_SWITCH_UPLOADS=false
KILL_SWITCH_PUBLIC_WRITE=false

# Logging
LOG_LEVEL=info
LOG_FORMAT=json